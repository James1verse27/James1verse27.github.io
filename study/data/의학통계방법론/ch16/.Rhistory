f_17_2 <- function(x){
n <- nrow(x)
sum_x <- sum(x[,1])
mean_x <- mean(x[,1])
sum_y <- sum(x[,2])
mean_y <- mean(x[,2])
sum_x2 <- sum(x[,1]^2)
sum_xy <- sum(x[,1]*x[,2])
b <- (sum_xy-((sum_x*sum_y)/n))/(sum_x2-(sum_x^2)/n)
a <- mean_y - b*mean_x
out <- data.frame(n,sum_x,mean_x,sum_y,mean_y,sum_x2,sum_xy,b,a)
return(out)
}
